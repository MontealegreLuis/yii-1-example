{% if items is not empty %}
    <ul>
        {% for item in items %}
            {% if item.visible is not defined or item.visible == true %}
                <li>
                    <a href="{{ app.createUrl(item.url, item.params|default([])) }}">{{ item.label }}</a>
                </li>
            {% endif %}
        {% endfor %}
    </ul>
{% endif %}
